{
  "name": "studioespresso/defaulttab",
  "description": "Add a standard tab to your new sections",
  "version": "1.0.0",
  "type": "craft-plugin",
  "keywords": [
    "cms",
    "craftcms",
    "utility",
    "yii2"
  ],
  "authors": [
    {
      "name": "Studio Espresso",
      "homepage": "https://studioespresso.co"
    }
  ],
  "support": {
    "email": "support@studioespresso.co",
    "issues": "https://studioespresso.co",
    "source": "https://studioespresso.co",
    "docs": "https://studioespresso.co"
  },
  "require": {
    "craftcms/cms": "~3.0.0-beta.1"
  },
    "autoload": {
        "psr-4": {
          "studioespresso\\defaulttab\\": "src/"
        }
    },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "name": "Default Tab",
    "handle": "defaulttab",
    "class": "studioespresso\\defaulttab\\DefaultTab"
  }
}
